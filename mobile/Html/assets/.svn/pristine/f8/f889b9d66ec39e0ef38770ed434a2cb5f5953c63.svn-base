<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1,user-scalable=no">
<title>资金流水</title>
<link rel="stylesheet" type="text/css" href="static/css/bootstrap.min.css" />
<link href="css/style.css" rel="stylesheet" type="text/css">
<script type="application/javascript" src="js/fastclick.js"></script>
<script type="application/javascript">
    window.addEventListener('load', function () {
        FastClick.attach(document.body);
    }, false);
</script>
</head>

	<body>
		<div>
			<!-- Nav tabs -->
			<ul class="nav nav-tabs nav-record" role="tablist">
				<li role="presentation" class="active"><a href="#apply" aria-controls="home" role="tab" data-toggle="tab">投资</a></li>
				<li role="presentation"><a href="#redeem" aria-controls="profile" role="tab" data-toggle="tab">赎回</a></li>
				<li role="presentation"><a href="#recharge" aria-controls="messages" role="tab" data-toggle="tab">充值</a></li>
				<li role="presentation"><a href="#withdraw" aria-controls="messages" role="tab" data-toggle="tab">提现</a></li>
			</ul>
			<!-- Tab panes -->
			<div class="tab-content">

				<div role="tabpanel" class="tab-pane active" id="apply">
					<table class="table table_record">
						<thead>
							<tr>
								<th>日期</th>
								<th>转入金额</th>
								<th>账户余额</th>
								<th>备注</th>
							</tr>
						</thead>
						<tbody id="applyList">

						</tbody>
					</table>
				</div>


				<div role="tabpanel" class="tab-pane" id="redeem">
					<table class="table table_record">
						<thead>
							<tr>
								<th>日期</th>
								<th>赎回金额</th>
								<th>账户余额</th>
								<th>备注</th>
							</tr>
						</thead>
						<tbody id="redeemList">

						</tbody>
					</table>
				</div>


				<div role="tabpanel" class="tab-pane" id="recharge">
					<table class="table table_record">
						<thead>
							<tr>
								<th>日期</th>
								<th>充值金额</th>
								<th>账户余额</th>
								<th>备注</th>
							</tr>
						</thead>
						<tbody id="rechargeList">

						</tbody>
					</table>
				</div>

				<div role="tabpanel" class="tab-pane" id="withdraw">
					<table class="table table_record">
						<thead>
						<tr>
							<th>日期</th>
							<th>提现金额</th>
							<th>账户余额</th>
							<th>备注</th>
						</tr>
						</thead>
						<tbody id="withdrawList">

						</tbody>
					</table>
				</div>

			</div>
		</div>

		<script src="static/js/jquery-2.0.3.min.js"></script>
		<script src="static/js/bootstrap.min.js"></script>
        <script src="js/common.js" charset="utf-8"></script>
		<script src="js/formatutil.js"  charset="utf-8"></script>
		<script src="js/tradelog.js"  charset="utf-8"></script>
	</body>

</html>